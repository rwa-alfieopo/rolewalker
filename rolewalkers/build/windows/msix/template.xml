<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="rolewalkers.exe"
        Arguments=""
        InstallLocation="C:\Program Files\RoleWalkers\RoleWalkers">
    </Installer>
    <PackageInformation
        PackageName="RoleWalkers"
        PackageDisplayName="RoleWalkers"
        PublisherName="CN=RoleWalkers"
        PublisherDisplayName="RoleWalkers"
        Version="1.0.0.0"
        PackageDescription="AWS SSO Profile and Account Manager">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.rolewalkers.app"
                Description="AWS SSO Profile and Account Manager"
                DisplayName="RoleWalkers"
                ExecutableName="rolewalkers.exe"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>RoleWalkers</DisplayName>
            <PublisherDisplayName>RoleWalkers</PublisherDisplayName>
            <Description>AWS SSO Profile and Account Manager</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="rolewalkers.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
